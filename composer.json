{
	"name": "prawee/yii2-widgets",
	"description": "yii2 widgets alert ",
	"type": "yii2-extension",
	"keywords": ["yii2","extension","alert"],
	"license": "MIT",
	"authors": [
		{
			"name": "prawee wongsa",
			"email": "konkeanweb@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"yiisoft/yii2": "2.*",
		"yiisoft/yii2-bootstrap4": "2.0.9"
	},
	"autoload": {
		"psr-4": {
			"prawee\\widgets\\": "src"
		}
	},
	"config": {
		"process-timeout": 1800,
		"discard-changes": true,
		"fxp-asset": {
			"enabled": false
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
